<%- include('./partials/head') %>
    <main class="all">
        <% data.forEach(element=> {%>
            <figure class="shop-article">
                <a href="/add/<%= element._id %> ">
                    <img src=<%=element.ProductLink %> alt="">
                </a>
                <figcaption>
                    <div>
                        <h2>
                            <%= element.ProductName %>
                        </h2>
                        <span>
                            <%= element.Company%>
                        </span>
                    </div>
                    <p>
                        <%= element.Price %>
                    </p>

                </figcaption>
            </figure>
            <%}); %>
    </main>
    <%- include('./partials/footer') %>